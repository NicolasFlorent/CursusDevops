
public class DessinEtoile {
	
	
	public static void ligne(int a) {
		for(int i=0;i<a;i++)
			System.out.print("*");
	}
	
	public static void rectangle(int a, int b) {
		for(int i=0;i<a;i++) {
			for(int j =0; j<b;j++){
				System.out.print("*");	
			}
		}
	}
	

	public static void main(String[] args) {
		
	}

}
